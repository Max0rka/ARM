                    </StackPanel>
                </StackPanel>
            </materialDesign:ColorZone>
            <materialDesign:ColorZone BorderBrush="Black" BorderThickness="1" Margin="0,7,0,0">
                <StackPanel>
                    <TextBlock Text="По АЛ (АКП)"/>
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="Всего" Margin="5" VerticalAlignment="Center" HorizontalAlignment="Left" Width="160"/>
                        <TextBox x:Name="TbGeneralRescAL"  Width="70" HorizontalAlignment="Left" VerticalAlignment="Center" PreviewTextInput="TbFloor_PreviewTextInput"/>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="В том числе детей" Margin="5" VerticalAlignment="Center" HorizontalAlignment="Left" Width="160"/>
                        <TextBox x:Name="TbResALChild"  Width="70" HorizontalAlignment="Left" VerticalAlignment="Center" PreviewTextInput="TbFloor_PreviewTextInput"/>
                    </StackPanel>
                </StackPanel>
            </materialDesign:ColorZone>
            <materialDesign:ColorZone BorderBrush="Black" BorderThickness="1" Margin="0,7,0,0">
                <StackPanel>
                    <TextBlock Text="С применением ППСУ-20"/>
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="Всего" Margin="5" VerticalAlignment="Center" HorizontalAlignment="Left" Width="160"/>
                        <TextBox x:Name="TbGeneralRescPPSY"  Width="70" HorizontalAlignment="Left" VerticalAlignment="Center" PreviewTextInput="TbFloor_PreviewTextInput"/>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="В том числе детей" Margin="5" VerticalAlignment="Center" HorizontalAlignment="Left" Width="160"/>
                        <TextBox x:Name="TbResPPSYChild"  Width="70" HorizontalAlignment="Left" VerticalAlignment="Center" PreviewTextInput="TbFloor_PreviewTextInput"/>
                    </StackPanel>
                </StackPanel>
            </materialDesign:ColorZone>
            <materialDesign:ColorZone BorderBrush="Black" BorderThickness="1" Margin="0,7,0,0">
                <StackPanel>
                    <TextBlock Text="С применением натяжного полотна"/>
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="Всего" Margin="5" VerticalAlignment="Center" HorizontalAlignment="Left" Width="160"/>
                        <TextBox x:Name="TbGeneralRescCanvas"  Width="70" HorizontalAlignment="Left" VerticalAlignment="Center" PreviewTextInput="TbFloor_PreviewTextInput"/>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="В том числе детей" Margin="5" VerticalAlignment="Center" HorizontalAlignment="Left" Width="160"/>
                        <TextBox x:Name="TbResCanvasChild"  Width="70" HorizontalAlignment="Left" VerticalAlignment="Center" PreviewTextInput="TbFloor_PreviewTextInput"/>
                    </StackPanel>
                </StackPanel>
            </materialDesign:ColorZone>
            <materialDesign:ColorZone BorderBrush="Black" BorderThickness="1" Margin="0,7,0,0">
                <StackPanel>
                    <TextBlock Text="Через окно"/>
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="Всего" Margin="5" VerticalAlignment="Center" HorizontalAlignment="Left" Width="160"/>
                        <TextBox x:Name="TbGeneralRescWindow"  Width="70" HorizontalAlignment="Left" VerticalAlignment="Center" PreviewTextInput="TbFloor_PreviewTextInput"/>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="В том числе детей" Margin="5" VerticalAlignment="Center" HorizontalAlignment="Left" Width="160"/>
                        <TextBox x:Name="TbResWindowChild"  Width="70" HorizontalAlignment="Left" VerticalAlignment="Center" PreviewTextInput="TbFloor_PreviewTextInput"/>
                    </StackPanel>
                </StackPanel>
            </materialDesign:ColorZone>
            <materialDesign:ColorZone BorderBrush="Black" BorderThickness="1" Margin="0,7,0,0">
                <StackPanel>
                    <TextBlock Text="Иное"/>
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="Всего" Margin="5" VerticalAlignment="Center" HorizontalAlignment="Left" Width="160"/>
                        <TextBox x:Name="TbGeneralRescOther"  Width="70" HorizontalAlignment="Left" VerticalAlignment="Center" PreviewTextInput="TbFloor_PreviewTextInput"/>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="В том числе детей" Margin="5" VerticalAlignment="Center" HorizontalAlignment="Left" Width="160"/>
                        <TextBox x:Name="TbResOtherChild"  Width="70" HorizontalAlignment="Left" VerticalAlignment="Center" PreviewTextInput="TbFloor_PreviewTextInput"/>
                    </StackPanel>
                </StackPanel>
            </materialDesign:ColorZone>
        </StackPanel>
        <ScrollViewer x:Name="SViewerAlignmentForce" Grid.Column="1" Visibility="Collapsed">
            <StackPanel x:Name="SPanelAlignmentForce" VerticalAlignment="Top" Grid.Column="1" Margin="10" Visibility="Visible">
                <ListView x:Name="ListViewCombatDepForces" BorderThickness="0">
                    <ListView.ItemTemplate>
                        <DataTemplate>
                            <CheckBox Content="{Binding CombatDepForceName}" IsChecked="{Binding IsCombatDepForceChecked}"/>
                        </DataTemplate>
                    </ListView.ItemTemplate>
                    <ListView.ItemContainerStyle>
                        <Style TargetType="ListViewItem">
                            <Setter Property="BorderThickness" Value="0"/>
                            <Setter Property="Margin" Value="1"/>
                        </Style>
                    </ListView.ItemContainerStyle>
                </ListView>
                <ListView x:Name="ListViewAlignmentForces" BorderThickness="0">
                    <ListView.ItemTemplate>
                        <DataTemplate>
                            <CheckBox Content="{Binding AlignmentFoceName}" IsChecked="{Binding IsAlignmentFoceChecked}"/>
                        </DataTemplate>
                    </ListView.ItemTemplate>
                    <ListView.ItemContainerStyle>
                        <Style TargetType="ListViewItem">
                            <Setter Property="BorderThickness" Value="0"/>
                            <Setter Property="Margin" Value="1"/>
                        </Style>
                    </ListView.ItemContainerStyle>
                </ListView>
            </StackPanel>
        </ScrollViewer>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Margin="10" Grid.ColumnSpan="2" Grid.Row="1" >
            <Button x:Name="BtnSave" IsEnabled="False" Content="{materialDesign:PackIcon Kind=Check}" Style="{StaticResource MaterialDesignFlatLightBgButton}" Click="BtnSave_Click"/>
            <Button x:Name="BtnCancel" Content="{materialDesign:PackIcon Kind=Cancel}" Style="{StaticResource MaterialDesignFlatLightBgButton}" Margin="20,0" Click="BtnCancel_Click"/>
        </StackPanel>
    </Grid>
</Window>
